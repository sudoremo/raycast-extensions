{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "xpass",
  "title": "xpass",
  "description": "Fill usernames, passwords and OTP tokens from pass.",
  "icon": "command-icon.png",
  "author": "remo_fritzsche",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "fill",
